{
  "name": "gatsby-source-cosmicjs",
  "version": "1.0.5",
  "description": "Gatsby Source Plugin for building websites using Cosmic JS as a data source",
  "author": {
    "email": "tspiro@tonyspiro.com",
    "name": "Tony Spiro",
    "url": "https://cosmicjs.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cosmicjs/gatsby-source-cosmicjs.git"
  },
  "bugs": {
    "url": "https://github.com/cosmicjs/gatsby-source-cosmicjs/issues"
  },
  "license": "MIT",
  "homepage": "https://cosmicjs.com",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src --out-dir . --ignore **/__tests__",
    "test": "jest",
    "format": "prettier --write 'src/**/*.js'",
    "watch": "babel -w src --out-dir ."
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "cosmicjs"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "lodash": "^4.17.11",
    "query-string": "^6.8.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "babel-preset-gatsby-package": "^0.2.3",
    "jest": "^24.9.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2"
  },
  "peerDependencies": {
    "gatsby": ">=2.13.26"
  },
  "pre-commit": [
    "format"
  ]
}
